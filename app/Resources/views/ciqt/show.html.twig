{% extends 'PetkoparaCrudGeneratorBundle::base.html.twig' %}

{% block title %}
{{ parent() }} - ciqT
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>Affichage du ciqT <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p style="text-decoration: underline black;"><strong>Techf:</strong></p>
        <p>
            {{ ciqT.techF }}
        </p>

    </div>
    <div  class="col-md-6">
        <p style="text-decoration: underline black;"><strong>Sitecodef:</strong></p>
        <p>
            {{ ciqT.siteCodeF }}
        </p>

    </div>
    <div class="col-md-6">
        <p style="text-decoration: underline black;"><strong>Integrationtypef:</strong></p>
        <p>
            {{ ciqT.integrationTypeF }}
        </p>

    </div>
    <div class="col-md-6">
        <p style="text-decoration: underline black;"> <strong>Sourcevendorf:</strong></p>
        <p>
            {{ ciqT.sourceVendorF }}
        </p>

    </div>
    <div class="col-md-6">
        <p style="text-decoration: underline black;"><strong>Regionf:</strong></p>
        <p>
            {{ ciqT.regionF }}
        </p>

    </div>
    <div class="col-md-6">
        <p style="text-decoration: underline black;"><strong>Targetcellnamef:</strong></p>
        <p>
            {{ ciqT.targetCellNameF }}
        </p>

    </div>
    <div class="col-md-6">
        <p style="text-decoration: underline black;"><strong>Dateallumagef:</strong></p>
        <p>
            {% if ciqT.dateAllumageF %}{{ ciqT.dateAllumageF|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p style="text-decoration: underline black;"><strong>Id:</strong></p>
        <p>
            {{ ciqT.id }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('ciqt') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('ciqt_edit', { 'id':  ciqT.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('ciqt_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('ciqt_delete', { 'id': ciqT.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>


{% endblock %}

